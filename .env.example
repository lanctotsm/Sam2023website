GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URL=http://localhost:8080/auth/callback
# Base admin email - automatically added to allowed_emails table on first startup
# Additional admins can be added via the API after authentication
BASE_ADMIN_EMAIL=
FRONTEND_BASE_URL=http://localhost:3000
S3_REGION=us-east-1
S3_BUCKET=cms
# Local MinIO public URL (bucket prefix required for direct access)
S3_PUBLIC_BASE_URL=http://localhost:9000/cms
# Optional S3-compatible endpoints (for local MinIO)
S3_ENDPOINT_URL=http://minio:9000
S3_PRESIGN_ENDPOINT_URL=http://localhost:9000
S3_FORCE_PATH_STYLE=true
# Local MinIO credentials (match MINIO_ROOT_* below)
AWS_USE_STATIC_CREDS=true
AWS_ACCESS_KEY_ID=minioadmin
AWS_SECRET_ACCESS_KEY=minioadmin
# Dev-only: enable /auth/dev-login so you can upload without Google OAuth
DEV_AUTH_BYPASS=true
# Show the Dev login button in the frontend
NEXT_PUBLIC_DEV_LOGIN=true
ALLOWED_ORIGINS=http://localhost:3000

# Local MinIO admin credentials
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
